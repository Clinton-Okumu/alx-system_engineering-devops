#!/usr/bin/env bash

# Find and kill the 7-highlander process without using kill or killall
process_name="7-highlander.sh"
pid=$(pgrep -f "$process_name")

if [ -n "$pid" ]; then
    echo "Killing $process_name process with PID $pid"
    # Send SIGKILL signal to forcefully kill the process
    kill -9 "$pid"
else
    echo "$process_name process not found"
fi
